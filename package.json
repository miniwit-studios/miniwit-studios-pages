{
    "name": "miniwit-studios-pages",
    "version": "0.0.0",
    "description": "Angular frontend for the site \"https://www.miniwitstudios.com\"",
    "scripts": {
        "lint": "bash ./scripts/lint.sh",
        "build": "yarn copy-assets && bash ./scripts/build.sh",
        "watch": "yarn copy-assets && bash ./scripts/watch.sh",
        "dev-server": "bash ./scripts/dev-server.sh",
        "start": "bash ./scripts/start.sh",
        "copy-assets": "yarn clean-dist && cpr public dist && cpr dist/index.html dist/200.html",
        "clean-dist": "rimraf dist && mkdirp dist",
        "test": "bash ./scripts/test.sh",
        "test-once": "bash ./scripts/test.sh once",
        "test-watch": "bash ./scripts/test.sh watch",
        "travis": "bash ./scripts/travis.sh"
    },
    "author": {
        "name": "Brandon Slade",
        "email": "brandonyoyoslade@gmail.com",
        "url": "http://brandonslade.me"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/jasmine": "^2.5.46",
        "jasmine-core": "^2.5.2",
        "karma": "^1.5.0",
        "karma-cli": "^1.0.1",
        "karma-defer-spec-reporter": "^0.0.30",
        "karma-jasmine": "^1.1.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-super-dots-reporter": "^0.1.0",
        "karma-webpack": "^2.0.3",
        "phantomjs-prebuilt": "^2.1.14",
        "tslint": "^5.1.0",
        "webpack-dev-server": "^2.4.2",
        "yargs": "^7.0.2"
    },
